                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT HubCustomers.CNP_or_CodeFiscal, HubComputers.ComputerName, HubLocations.Address, HubPurposeOfSale.PurposeID, SatSales.Amount, SatSales.Price, SatSales.[Date]
FROM     LinkResidence INNER JOIN
                  HubCustomers ON LinkResidence.H_Customers_SQN = HubCustomers.H_Customers_SQN INNER JOIN
                  HubLocations ON LinkResidence.H_Locations_SQN = HubLocations.H_Locations_SQN INNER JOIN
                  LinkSales ON HubCustomers.H_Customers_SQN = LinkSales.H_Customer_SQN INNER JOIN
                  HubPurposeOfSale ON LinkSales.H_Purpose_of_sale_SQN = HubPurposeOfSale.H_Purpose_of_sale_SQN INNER JOIN
